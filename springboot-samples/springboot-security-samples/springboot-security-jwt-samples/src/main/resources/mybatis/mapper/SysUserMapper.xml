<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.chenzw.springboot.security.jwt.repository.SysUserMapper">


    <select id="findByUsername" resultType="cn.chenzw.springboot.security.jwt.domain.entity.SysUser">
        select * from sys_user where username = #{username}
    </select>

</mapper>